{
  "manifest_version": 2,
  "name": "Nustry",
  "version": "1.0",
  "description": "Search with confidence, Nustry works for your privacy.",
  "icons": {
      "96": "icon.png"
   },
   "background": {
      "scripts": ["background.js"],
      "persistent": false
   },
   "browser_action": {
      "default_icon": "icon.png"
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "permissions": [ "tabs" ]
}
